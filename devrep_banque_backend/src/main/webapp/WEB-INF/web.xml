<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>CompteServlet</servlet-name>
  	<display-name>CompteServlet</display-name>
  	<description></description>
  	<servlet-class>fr.su.jee.servlet.CompteServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>InitServlet</servlet-name>
  	<display-name>InitServlet</display-name>
  	<description></description>
  	<servlet-class>fr.su.jee.servlet.InitServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DecouvertServlet</servlet-name>
  	<display-name>DecouvertServlet</display-name>
  	<description></description>
  	<servlet-class>fr.su.jee.servlet.DecouvertServlet</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>CompteServlet</servlet-name>
  	<url-pattern>/CompteServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>InitServlet</servlet-name>
  	<url-pattern>/InitServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DecouvertServlet</servlet-name>
  	<url-pattern>/DecouvertServlet</url-pattern>
  </servlet-mapping>
</web-app>
